                        </tr>
                        <tr>
                            <td>{{ odstep_6_spacji|raw }}Budownictwo</td>
                            <td class="text-center">{{ dane_zagregowane.pozyczki_wg_dzialanosci.liczba_pozyczek_dzialania_budownicze|default('0') }}</td>
                            <td class="text-center decimal-11-2">{{ dane_zagregowane.pozyczki_wg_dzialanosci.kwota_pozyczek_dzialania_budownicze|default('0.00') }}</td>
