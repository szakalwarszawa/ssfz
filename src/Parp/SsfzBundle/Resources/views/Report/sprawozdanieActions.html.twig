<div class="row pull-right">
    <a href="{{ path('sprawozdanie_podglad', {'reportId': dt_obj.id })}}" target="_blank" class="btn btn-sm btn-info" role="button"><span class="glyphicon glyphicon-eye-open"></span> Podgląd</a>
    {% if dt_obj.status == 1 %}
        <a href="/sprawozdanie/edycja/{{dt_obj.umowaId}}/{{dt_obj.id}}" class="btn btn-sm btn-primary" role="button"><span class="glyphicon glyphicon-edit"></span> Edytuj</a>
        <a href="/przeplyw/rejestracja/{{dt_obj.id}}" class="btn btn-sm btn-success" role="button"><span class="glyphicon glyphicon-plus"></span> Dodaj przeływy finansowe</a>
        <span class="btn btn-sm btn-warning btn-send-to-parp" row-id="{{dt_obj.id}}" onclick="send({{dt_obj.id}})" role="button"><span class="glyphicon glyphicon-share-alt"></span> Prześlij do PARP</span>
    {% else %}
         {% if dt_obj.status == 4 %}
             <a href="/sprawozdanie/poprawa/{{dt_obj.umowaId}}/{{dt_obj.id}}" class="btn btn-sm btn-primary" role="button"><span class="glyphicon glyphicon-edit"></span> Popraw</a>
         {% else %}
             <span class="btn btn-sm btn-danger" ><span class="glyphicon glyphicon-upload"></span>&nbsp Przesłany &nbsp</span>
         {% endif %}
    {% endif %}
</div>

